<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.zeami.ennosuke.falloutterminalhacker" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.zeami.ennosuke.falloutterminalhacker.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.zeami.ennosuke.falloutterminalhacker.Hacking"
            android:label="@string/title_activity_hacking"
            android:parentActivityName="de.zeami.ennosuke.falloutterminalhacker.MainActivity" >
        </activity>
        <activity
            android:name="de.zeami.ennosuke.falloutterminalhacker.MainHelp"
            android:label="@string/title_activity_main_help"
            android:parentActivityName="de.zeami.ennosuke.falloutterminalhacker.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="de.zeami.ennosuke.falloutterminalhacker.MainActivity" />
        </activity>
    </application>

</manifest>
